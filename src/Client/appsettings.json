{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "CallbackPath": "/signin-oidc",
    "Authority": "https://login.microsoftonline.com/{TenantId}",
    "TenantId": "{TenantId}",
    "ClientId": "{ClientId}",
    "ClientSecret": "{ClientSecret}",
    "ValidateAuthority": true,
    "Instance": "https://login.microsoftonline.com/",
    "ResponseType": "code"
  },
  "AzureACR": {
    "Name": "{ACRHostName}",
    "BaseUrl": "{ACRURL}"
  }
}
